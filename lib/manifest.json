{
	"default-remote":"mootools",
	"remotes": {
		"anutron": {
			"fetch": "git://github.com/anutron/%(name)s.git"
		},
		"mootools": {
			"fetch": "git://github.com/mootools/%(name)s.git"
		}
	},
	"projects": {
		"mootools-core": { 
			"dir":"core",
			"refspec": "1.2x"
		},
		"mootools-more": {
			"dir":"more",
			"refspec": "1.2x"
		},
		"mootools-depender": {
			"dir":"depender",
			"refspec": "master",
			"from-remote": "anutron",
			"remotes": ["anutron"]
		},
		"mootools-test-runner": {
			"dir":"test-runner",
			"refspec": "master",
			"from-remote": "anutron",
			"remotes": ["anutron"]
		}
	}
}
