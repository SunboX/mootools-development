{
	"default-remote":"anutron",
	"remotes": {
		"anutron": {
			"fetch": "git://github.com/anutron/%(name)s.git"
		}
	},
	"projects": {
		"mootools-core": { 
			"dir":"core",
			"track-indirect": "core.hash"
		},
		"mootools-more": {
			"dir":"more",
			"track-indirect": "more.hash"
		},
		"mootools-depender": {
			"dir":"depender",
			"track-branch": "arian"
		},
		"mootools-test-runner": {
			"dir":"dev-app",
			"track-branch": "arian"
		},
		"mootools-runner": {
			"track-branch": "master"
		},
		"mootools-core-specs": {
			"dir":"core-specs",
			"track-branch": "master"
		}
	}
}
