{
	"default-remote":"cloudera",
	"remotes": {
		"cloudera": {
			"fetch": "git://github.com/cloudera/%(name)s.git"
		}
	},
	"projects": {
		"mootools-core": { 
			"dir":"core",
			"track-indirect": "core.hash"
		},
		"mootools-more": {
			"dir":"more",
			"track-indirect": "more.hash"
		},
		"mootools-depender": {
			"dir":"depender",
			"track-branch": "master"
		},
		"mootools-test-runner": {
			"dir":"test-runner",
			"track-branch": "master"
		},
		"art": {
			"dir":"art",
			"track-indirect": "art.hash"
		},
		"behavior": {
			"track-branch": "master"
		},
		"clientcide": {
			"track-indirect": "clientcide.hash"
		},
		"mootools-color": {
			"dir":"color",
			"track-indirect": "color.hash"
		},
		"FitText": {
			"dir":"fittext",
			"track-branch": "master"
		},
		"jframe": {
			"track-branch": "master"
		},
		"more-behaviors": {
			"track-branch": "master"
		},
		"slick": {
			"track-indirect": "slick.hash"
		},
		"mootools-touch": {
			"dir":"touch",
			"track-indirect": "touch.hash"
		},
		"mootools-table": {
			"dir":"table",
			"track-indirect": "table.hash"
		},
		"art-widgets": {
			"dir":"widgets",
			"track-branch": "master"
		}
	}
}
