{
	"default-remote":"cloudera",
	"remotes": {
		"cloudera": {
			"fetch": "git://github.com/cloudera/%(name)s.git"
		}
	},
	"projects": {
		"mootools-core": { 
			"dir":"core",
			"track-indirect": "core.hash"
		},
		"mootools-more": {
			"dir":"more",
			"track-indirect": "more.hash"
		},
		"mootools-depender": {
			"dir":"depender",
			"track-branch": "master"
		},
		"mootools-test-runner": {
			"dir":"test-runner",
			"track-branch": "master"
		},
		"behavior": {
			"track-branch": "master"
		},
		"more-behaviors": {
			"track-branch": "master"
		},
		"clientcide": {
			"track-branch": "master"
		},
		"cloudera-ui": {
			"track-branch": "master"
		},
		"mootools-runner": {
			"track-branch": "master"
		},
		"mootools-core-specs": {
			"dir":"core-specs",
			"track-branch": "master"
		}
	}
}
